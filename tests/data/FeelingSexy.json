{
	"meta_data": {
		"delimiter": "\t"
	},
	"product_mapping": {
		"id": {
			"Description":"Unique id of a product (should remain the same for this product)",
			"Actions": {
				"store": "retailerNumber"
			}
		},
		"title": {
			"Description":"The product full name",
			"Actions": {
				"store": "productname"
			}
		},
		"link": {
			"Actions": {
				"validate": {
					"type": "url",
					"endpoint": "http://www.feelingsexy.com.au/"
				},
				"store": "pageUrl"
			}
		},
		"price": {
			"Actions": {
				"store": "price",
				"store_meta": {
					"priceCurrency": {
						"value": "AUD",
						"type": "exact"
					}
				}
			}
		},
		"description": {
			"Description":"Description of the product",
			"Actions": {
				"store": "description"
			}
		},
		"condition": {
			"Actions": {
				"store": ""
			}
		},
		"gtin": {
			"Description":"Barcode of the product",
			"Actions": {
				"store": "gtin"
			}
		},
		"brand": {
			"Description": "Product brand",
			"Actions": {
				"store": "brandname"
			}
		},
		"gender": {
			"Description": "Desired gender",
			"Accepted Values": {
				"Unisex": "DB_SEX_UNISEX",
				"Male": "DB_SEX_MALE",
				"Female": "DB_SEX_FEMALE"
			},
			"Actions": {
				"convert": "const",
				"store": "gender"
			}
		},
		"image_link": {
			"Description":"Direct link to the item image",
			"Actions": {
				"validate": {
					"type": "url",
					"endpoint": "http://www.feelingsexy.com.au/images/products/allproducts/"
				},
				"store": "product_image"
			}
		},
		"google_product_category": {
			"Accepted Values": {
				"Perfume & Cologne": {
					"category": "DB_CAT_PERFUME"
				},
				"Hair Care": {
					"category": "DB_CAT_BEAUTY",
					"kindId": "DB_KIND_HAIR_CARE"
				},
				"Health & Beauty": {
					"category": "DB_CAT_BEAUTY"
				}
			},
			"Actions": {
				"gen_split": {
					"stop": " > "
				}
			}
		},
		"product_type": {
			"Actions": {
			}
		},
		"quantity": {
			"Actions": {
			}
		},
		"availability": {
			"Accepted Values": {
				"in stock": 1,
				"out of stock": 0,
				"preorder": 2
			},
			"Actions": {
				"convert": "exact",
				"store": "inStock"
			}
		},
		"size": {
			"Actions": {
				"store": "quantityStr"
			}
		},
		"shipping": {
			"Actions": {
				"ser_split": {
					"country" : {
						"stop": "::",
						"store": "shippingCountries"
					},
					"service": {
						"stop": ":",
						"store": "shippingService"
					},
					"price": {
						"store": "shippingPrice"
					}
				}
			}
		},
		"identifier_exists": {
			"Actions": {
			}
		}
	}
}
